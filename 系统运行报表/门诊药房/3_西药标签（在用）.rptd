{"ReportId":"a5c3d6fd-b1ae-81a9-8e05-bfd2ebfad6f9","RptParameter":"{\"S_JSSJ\":{\"ID\":\"S_JSSJ\",\"Used\":false,\"Name\":\"结束时间\",\"Type\":3,\"DisplayModel\":0,\"DefaultValue\":\"本月末时间\",\"RptPar\":false,\"Config\":null,\"IsHide\":false},\"S_KSSJ\":{\"ID\":\"S_KSSJ\",\"Used\":false,\"Name\":\"开始时间\",\"Type\":3,\"DisplayModel\":0,\"DefaultValue\":\"本月初时间\",\"RptPar\":false,\"Config\":null,\"IsHide\":false},\"S_DQY\":{\"ID\":\"S_DQY\",\"Used\":false,\"Name\":\"当前页\",\"Type\":2,\"DisplayModel\":0,\"DefaultValue\":\"1\",\"RptPar\":false,\"Config\":null,\"IsHide\":false},\"S_ZYS\":{\"ID\":\"S_ZYS\",\"Used\":false,\"Name\":\"总页数\",\"Type\":2,\"DisplayModel\":0,\"DefaultValue\":\"1\",\"RptPar\":false,\"Config\":null,\"IsHide\":false},\"S_CZY\":{\"ID\":\"S_CZY\",\"Used\":false,\"Name\":\"操作员\",\"Type\":1,\"DisplayModel\":0,\"DefaultValue\":\"\",\"RptPar\":false,\"Config\":null,\"IsHide\":false},\"S_CZYBM\":{\"ID\":\"S_CZYBM\",\"Used\":false,\"Name\":\"操作员编码\",\"Type\":1,\"DisplayModel\":0,\"DefaultValue\":\"\",\"RptPar\":false,\"Config\":null,\"IsHide\":false},\"CS\":{\"Order\":1,\"ID\":\"CS\",\"Used\":true,\"Name\":\"参数\",\"Type\":1,\"DisplayModel\":0,\"DefaultValue\":\"{\\\"RptCode\\\":\\\"p4\\\",\\\"PrintName\\\":\\\"choosePrint\\\",\\\"Storehouse_Id\\\":\\\"73\\\",\\\"Rcp_No\\\":\\\"23002060546\\\",\\\"Begin_Time\\\":\\\"2023-06-05T00:00\\\",\\\"End_Time\\\":\\\"2023-06-05T23:59\\\",\\\"Pati_Id\\\":\\\"2767079\\\",\\\"Pati_Page_Id\\\":\\\"312975\\\",\\\"Pat_Source\\\":1,\\\"Grdrug_Sign\\\":1,\\\"Already_Grdrug_Sign\\\":1,\\\"Grdrug_Id\\\":\\\"241846f8-85b5-402e-88fc-d710773ea930\\\",\\\"Query_Easai_Sign\\\":0}\",\"Format\":\"\",\"RptPar\":true,\"Config\":null,\"IsHide\":false,\"IsDataSetPar\":false}}","RptDataset":"[{\"ID\":\"e0f9dae6-c939-701e-a7d1-f650b133990f\",\"Name\":\"西药明细HIS\",\"UsedParametersId\":\"[\\\"CS\\\"]\",\"Configuration\":{\"AgentPars\":[{\"Name\":\"CS\",\"ParValue\":\"{P_CS}\",\"Format\":\"\",\"ParDirection\":1,\"ParType\":1}],\"DSEnum\":0,\"CmdTimeOut\":30,\"SQL\":\"select 医嘱ID,listagg(内容,chr(10)) within group(order by 1)   内容 from (Select id as 医嘱ID,\\n       'M' as 性质,\\n       药品 || '    X' || decode(记录性质,\\n                           1,\\n                           round(总数量 / 门诊包装, 3) || 门诊单位,\\n                           round(总数量 / 住院包装, 3) || 住院单位) As 内容\\n  From (Select A.id,\\n              I.名称|| '(' || I.规格||')' As 药品,\\n               A.记录性质,\\n               decode(A.有费用,\\n                      1,\\n                      总数量,\\n                      A.总剂量 / decode(T.剂量系数, 0, 1, Null, 1, T.剂量系数)) As 总数量,\\n               decode(T.门诊包装, 0, 1, Null, 1, T.门诊包装) As 门诊包装,\\n               T.门诊单位,\\n               decode(T.住院包装, 0, 1, Null, 1, T.住院包装) As 住院包装,\\n               T.住院单位\\n          From (Select A.相关id,\\n                       A.Id,\\n                       decode(E.收费细目id, Null, A.收费细目id, E.收费细目id) As 药品id,\\n                       decode(E.收费细目id, Null, 0, 1) As 有费用,\\n                       A.总剂量,\\n                       E.总数量,\\n                       A.记录性质\\n                  From (Select A.相关id,\\n                               A.Id,\\n                               A.收费细目id,\\n                               S.发送数次 As 总剂量,\\n                               S.记录性质\\n                          from 病人医嘱记录 A,\\n                               (Select S.医嘱id, S.发送数次, S.记录性质\\n                                  From 病人医嘱发送 S\\n                                 Where S.No = Pljson(:CS).Get_String('Rcp_No')  and s.记录性质=Pljson(:CS).Get_number('Pat_Source')) S\\n                         where A.ID = S.医嘱ID  and a.诊疗类别 in('5','6') \\n                 \\n         \\nand a.病人id=Pljson(:CS).Get_String('Pati_Id')) A,\\n                       (select E.医嘱序号   as id,\\n                               E.收费细目ID,\\n                               E.数次       As 总数量\\n                          from 门诊费用记录 E\\n                         where E.No = Pljson(:CS).Get_String('Rcp_No')                         \\n                          and E.记录性质=Pljson(:CS).Get_number('Pat_Source')\\n                           And E.执行状态 <> 9 AND E.收费类别 in('5','6')\\n                        union all\\n                        select E.医嘱序号   as id,\\n                               E.收费细目ID,\\n                               E.数次       As 总数量\\n                          from 住院费用记录 E\\n                         where E.No = Pljson(:CS).Get_String('Rcp_No')\\n                          and e.记录性质=Pljson(:CS).Get_number('Pat_Source')\\n                         AND E.收费类别 in('5','6')) E\\n                 Where A.Id = E.Id(+)) A,\\n               收费项目目录 I,\\n               药品规格 T\\n         Where A.药品id = I.id\\n           And A.药品id = T.药品id)\\nUnion All\\nSelect U.ID as 医嘱ID,\\n       'U' As 性质,\\n      K.医嘱内容||'  每次'||trim(to_char(U.单次用量,'fm99990.90'))||G.计算单位||'  '||CASE WHEN u.执行频次 = '一次性' THEN '每天一次' ELSE u.执行频次 END As 内容\\n  From 病人医嘱记录 U,病人医嘱记录 K,诊疗项目目录 G,\\n       (Select Distinct A.相关id\\n          from 病人医嘱记录 A,\\n               (Select S.医嘱id, S.发送数次, S.记录性质\\n                  From 病人医嘱发送 S\\n                 Where S.No = Pljson(:CS).Get_String('Rcp_No') \\n                  and s.记录性质=Pljson(:CS).Get_number('Pat_Source')\\n                 \\n       \\n      ) S\\n         where A.ID = S.医嘱ID and a.诊疗类别 in('5','6') and a.病人id=Pljson(:CS).Get_String('Pati_Id')) M\\n Where U.相关Id = M.相关id AND K.ID=M.相关id AND U.诊疗项目ID=G.ID and g.类别 in('5','6')  \\nunion all \\nSelect M.ID as 医嘱ID,\\n       'PS' As 性质,\\n       '药师提示：'||to_char(U.使用说明)  As 内容\\n  From 药品规格 U,\\n       (Select Distinct A.ID,A.收费细目ID\\n          from 病人医嘱记录 A,\\n               (Select S.医嘱id, S.发送数次, S.记录性质\\n                  From 病人医嘱发送 S\\n                 Where S.No = Pljson(:CS).Get_String('Rcp_No')  \\n                   and s.记录性质=Pljson(:CS).Get_number('Pat_Source')\\n\\n      ) S\\n         where A.ID = S.医嘱ID and a.诊疗类别 in('5','6') and a.病人id=Pljson(:CS).Get_String('Pati_Id')) M\\n Where U.药品Id = M.收费细目id \\n\\n Order By 医嘱ID, 性质) group by 医嘱ID\",\"MaxRowCount\":-1},\"DataFields\":[{\"ColumnName\":\"医嘱ID\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"内容\",\"BizType\":1,\"Children\":[]}],\"DsId\":\"7496ecc7-7ced-4603-afa8-db9b236c3b34\",\"IsCommon\":false,\"IsLocalDataSet\":false,\"UsedExpressionID\":\"[]\"},{\"ID\":\"ca57711a-37dc-daf2-56d5-2a1b3fd33f4a\",\"Name\":\"信息HIS\",\"UsedParametersId\":\"[\\\"CS\\\"]\",\"Configuration\":{\"AgentPars\":[{\"Name\":\"CS\",\"ParValue\":\"{P_CS}\",\"Format\":\"\",\"ParDirection\":1,\"ParType\":1}],\"DSEnum\":0,\"CmdTimeOut\":30,\"SQL\":\"select distinct 姓名,性别,年龄,标识号,NO AS 流水号,开单科室,执行科室,开单人,开单人编号,审核人,配药人,核对人,发药人,地址,诊断,电话\\nfrom (\\nselect \\na.姓名,\\na.性别,\\na.年龄,\\nd.门诊号 as 标识号,\\na.NO,\\nb.名称 as 开单科室,\\nc.名称 as 执行科室,\\na.填制人 as 开单人,\\ne.编号 as 开单人编号,\\na.审核人 as 审核人,\\na.配药人 as 配药人,\\na.核查人 as 核对人,\\na.取药确认人员 as 发药人,\\nd.家庭地址 as 地址,\\na.诊断描述 as 诊断,\\nd.联系人电话 as 电话\\n from  药品收发记录 a,部门表 b,部门表 c,病人信息 d,人员表 e\\nwhere a.病人id = d.病人id\\nand a.病人科室id=b.id\\nand d.病人id = d.病人id\\nand a.库房id = c.id\\nand a.填制人=e.姓名\\nand a.NO=Pljson(:CS).Get_String('Rcp_No') \\n AND A.库房ID=Pljson(:CS).Get_String('Storehouse_Id') \\n and a.病人id=Pljson(:CS).Get_String('Pati_Id') /*and a.记录性质=e.病人来源 */\\ngroup by  a.姓名,a.性别,a.年龄,d.门诊号,a.NO,b.名称 ,c.名称 ,a.填制人,e.编号,a.审核人,a.配药人,a.核查人,a.取药确认人员,d.家庭地址,a.诊断描述,d.联系人电话\\n\\nUNION ALL\\n\\nselect \\na.姓名,\\na.性别,\\na.年龄,\\nd.住院号 as 标识号,\\na.NO,\\nb.名称||'      床号:'||d.出院病床||'床' as 开单科室,\\nc.名称 as 执行科室,\\na.填制人 as 开单人,\\ne.编号 as 开单人编号,\\na.审核人 as 审核人,\\na.配药人 as 配药人,\\na.核查人 as 核对人,\\na.取药确认人员 as 发药人,\\nd.家庭地址 as 地址,\\na.诊断描述 as 诊断,\\nd.联系人电话 as 电话\\nfrom 药品收发记录 A,部门表 b,部门表 c,病案主页 d,人员表 e\\nwhere a.病人科室id=b.id \\nand a.库房id=c.id  \\nand a.病人id=d.病人id \\nand a.主页id=d.主页id \\nand a.填制人=e.姓名\\nand a.NO=Pljson(:CS).Get_String('Rcp_No') \\nAND A.库房ID=Pljson(:CS).Get_String('Storehouse_Id') \\nand a.病人id=Pljson(:CS).Get_String('Pati_Id') /*and a.记录性质=e.病人来源 */\\ngroup by  a.姓名,a.性别,a.年龄,d.住院号,a.NO,b.名称 ,c.名称 ,A.填制人,e.编号,d.出院病床,a.审核人,a.配药人,a.核查人,a.取药确认人员,d.家庭地址,a.诊断描述,d.联系人电话)\",\"MaxRowCount\":-1},\"DataFields\":[{\"ColumnName\":\"姓名\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"性别\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"年龄\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"标识号\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"流水号\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"开单科室\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"执行科室\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"开单人\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"开单人编号\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"审核人\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"配药人\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"核对人\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"发药人\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"地址\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"诊断\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"电话\",\"BizType\":1,\"Children\":[]}],\"DsId\":\"7496ecc7-7ced-4603-afa8-db9b236c3b34\",\"IsCommon\":false,\"IsLocalDataSet\":false,\"UsedExpressionID\":\"[]\"},{\"ID\":\"78ea0453-a00a-cc57-c5dd-3b8e5488d535\",\"Name\":\"A\",\"UsedParametersId\":\"[\\\"CS\\\"]\",\"UsedExpressionID\":\"[]\",\"Configuration\":{\"AgentPars\":[{\"Name\":\"CS\",\"ParValue\":\"{P_CS}\",\"Format\":\"\",\"ParDirection\":1,\"ParType\":1}],\"DSEnum\":0,\"CmdTimeOut\":30,\"SQL\":\"select case \\nwhen instr(x.text,'流水号') > 0 then x.flag||'_1' \\nwhen instr(x.text,'药名') > 0 then x.flag||'_2'   \\nwhen instr(x.text,'嘱托') > 0 then x.flag||'_3' \\nwhen instr(x.text,'姓名') > 0 then x.flag||'_4'  \\nwhen instr(x.text,'用法') > 0 then x.flag||'_5' \\nwhen instr(x.text,'注意') > 0 then x.flag||'_6' \\nelse '999'\\nend as xh,\\n  x.flag \\n，replace(text ,'药名:','') as text \\n，日期 from \\n\\n(\\nWITH  AA AS \\n(\\nselect A.*, B.医生嘱托 ,开单日期, b.使用说明  from (\\n-- 修改此报表请注意：单量跟总量分开两个标签。\\n\\nSelect Distinct \\n---A.序号 ,\\nA.NO AS 流水号 \\n,A.姓名,A.性别,A.年龄\\n\\n--B.名称是药品名称，E.名称是药品别名\\n,F.名称 As 科室, A.药品ID, TRIM(Nvl(B.名称,E.名称)) As 药名, B.规格\\n,i.id as 医嘱序号\\n,(case when G.计算单位 in ('ml','滴','丸') or B.计算单位 in ('盒','瓶') then  '每次'||LTRIM(decode(substr(to_number(TO_CHAR(A.单量, '999999999990.00')),1,1),'.','0'||to_number(TO_CHAR(A.单量, '999999999990.00')),to_number(TO_CHAR(A.单量, '999999999990.00'))))||g.计算单位\\n        else  '每次'||LTRIM(decode(substr(to_number(TO_CHAR(A.单量/D.剂量系数, '999999999990.00')),1,1),'.','0'||to_number(TO_CHAR(A.单量/D.剂量系数, '999999999990.00')),to_number(TO_CHAR(A.单量/D.剂量系数, '999999999990.00'))))||b.计算单位 \\n        end) As 单量,\\n       (case when G.计算单位 ='ml' then  '共'||sum(A.实际数量/D.门诊包装)||Decode('D.门诊包装','D.门诊包装',D.门诊单位,D.住院单位) \\n        else  '共'||(sum(A.实际数量/D.门诊包装))||Decode('D.门诊包装','D.门诊包装',D.门诊单位,D.住院单位) end) As 发药量,\\n     (case when H.编码 in (002,003,004) then  '每天'||H.频率次数||'次' \\n           WHEN A.频次 = '一次性' THEN '每天一次' \\n           else  A.频次 end) As 频次,\\nA.用法,B.说明 As 备注,To_Char(Trunc(Sysdate), 'yyyy-mm-dd hh24:mi:ss') As 日期\\nFrom 药品收发记录 A, 收费项目目录 B, \\n药品规格 D, 收费项目别名 E, 部门表 F,诊疗项目目录 G,诊疗频率项目 H,病人医嘱记录 i, 病人医嘱发送 j\\nWhere A.药品id = B.ID \\nand j.医嘱ID = i.id\\nand i.收费细目ID = a.药品ID\\n--AND A.no=J.no\\n and a.医嘱id=J.医嘱id\\nAnd B.ID = D.药品id And B.ID = E.收费细目id(+) And A.对方部门id=F.ID And D.药名ID=G.id And A.频次=H.名称\\n    and d.发药类型 in ('口服类','外用类') And E.码类(+) = 1 And E.性质(+) = 3 \\n  -- 入参 1 药品收发记录 no\\n     And A.NO =Pljson(:CS).Get_String('Rcp_No')  \\n      AND A.库房ID=Pljson(:CS).Get_String('Storehouse_Id')  \\n      and A.病人id=Pljson(:CS).Get_String('Pati_Id')\\n     -- and a.医嘱id=Pljson(:CS).Get_String('Order_Id') \\n     --and A.NO ='22000013077'\\n     --select * from 药品收发记录 where no='22000013077'\\n     and g.类别 in ('5','6')\\n     \\n    --And (Mod(C.记录状态, 3) = 0 Or C.记录状态 = 1) \\nGroup by i.id\\n--,a.序号\\n,A.NO,a.姓名,a.性别,a.年龄,F.名称, A.药品ID, Nvl(B.名称,E.名称),B.规格\\n,G.计算单位,B.计算单位,A.单量,D.剂量系数,A.用法,A.频次,B.说明\\n,D.门诊单位,D.住院单位,H.编码,H.频率次数\\nOrder By 医嘱序号\\n\\n) A,\\n----- 取到药品数据后，整体关联一个医生嘱托。\\n(\\nselect distinct A.ID 医嘱的id, B.医嘱ID, d.药品ID, a.医生嘱托 ,to_char(A.开嘱时间,'yyyy-mm-dd') as 开单日期, to_char(d.使用说明) as 使用说明\\n  from  病人医嘱记录 A, 病人医嘱发送 B, 药品收发记录 C,药品规格 D\\n where B.NO(+) = c.No and B.医嘱ID = A.id and A.收费细目ID = C.药品ID\\n     -- 入参 1 药品收发记录 no\\n  And C.NO =Pljson(:CS).Get_String('Rcp_No')  \\n   AND C.库房ID=Pljson(:CS).Get_String('Storehouse_Id')  \\n   and C.病人id=Pljson(:CS).Get_String('Pati_Id') \\n  -- and c.医嘱id=Pljson(:CS).Get_String('Order_Id') \\n  --And b.记录性质 =1 \\n  --and C.NO ='22000013077'\\n   and a.诊疗项目id=d.药名ID\\n    and d.发药类型 in ('口服类','外用类')\\n   --And (Mod(C.记录状态, 3) = 0 Or C.记录状态 = 1) \\n   --And (C.用法 like '%口服%'  OR  C.用法 like '%外用%' or c.用法 like '%塞肛%' or c.用法 like '%喷喉%' or c.用法 like '%滴鼻%')\\n   and C.单据 in( 8,9,10) \\n) B\\n where A.药品ID = B.药品ID(+) \\n And A.医嘱序号 = B.医嘱的id(+)\\n order by 医嘱序号\\n \\n )\\n --流水号  日期\\n SELECT 医嘱序号  AS FLAG,'流水号:'||流水号 AS text ,null 性别 ,'日期:'||开单日期  as 日期 from AA\\n --药品 总量\\n UNION ALL \\n SELECT 医嘱序号  AS FLAG,'药名:'||药名 AS text,null 性别 ,发药量 from AA\\n --嘱托\\n  UNION ALL \\n SELECT  医嘱序号  AS FLAG,'嘱托:'||医生嘱托 AS text,null 性别 ,null from AA\\n  --患者信息，给药途径\\n  UNION ALL \\n SELECT  医嘱序号  AS FLAG,'姓名:'||姓名||'<'||性别||'>' AS text ,null ,用法 from AA\\n --服法，单量\\n   UNION ALL \\n SELECT  医嘱序号  AS FLAG,'用法:'||CASE WHEN AA.频次 = '一次性' THEN '每天一次' ELSE AA.频次 END AS text ,null, 单量 from AA\\n --特殊用药说明\\n   union all\\n select 医嘱序号 as flag, '注意:' || to_char(aa.使用说明)  as text, null, null from aa\\n ) x\\n order by xh\\n \",\"MaxRowCount\":-1},\"DataFields\":[{\"ColumnName\":\"XH\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"FLAG\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"TEXT\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"日期\",\"BizType\":1,\"Children\":[]}],\"DsId\":\"7dd4878f-6f5e-4464-bc5e-389cc88e4bfe\",\"IsCommon\":false,\"IsLocalDataSet\":false,\"DsType\":0}]","RptContent":"{\"LayoutInfo\":\"{\\\"LayoutName\\\":\\\"Layout1\\\",\\\"PageSzie\\\":{\\\"pageName\\\":\\\"自定义大小\\\",\\\"PageType\\\":256,\\\"Width\\\":10000,\\\"Height\\\":8800,\\\"IsLandScape\\\"