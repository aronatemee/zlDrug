hildren\":[]},{\"ColumnName\":\"Crxdtl_Tchmrfn\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Crx_Qunt\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"AllUnitQuantity\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Returned_Qunt\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"Allow_Return_Qunt\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"Return_Qunt\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"Grdrug_Id_His\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Outpatient_Unit\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Outpatient_Unit_Content\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Rcp_No\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Storehouse_Id\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Apply_Time\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"New_Fee_Sign\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"BillVoidId\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"MedicalInsuranceCode\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"MedicalInsuranceName\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Drug_Spell_Name\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Document_Type\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Pati_Page_Id\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"PatiSigns\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"PatiAllergy\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Medication_Frequency\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"DrugStock_Quantity\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Crxgsth_Id\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Grdrug_Detail_Id\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Grdrug_Id\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Drug_Id\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Tchm_Type\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Tchm_Strength\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Tchm_Strength_Type\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Tchm_Prdc\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Retail_Price\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Retail_Chrg\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Storehouse_Unit_Quantity\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"Storehouse_Unit\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Min_Unit\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Min_Unit_Quantity\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Lot_No\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Exp_Date\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Batch_No\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Grdrug_Sign\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"Bill_Type\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Diagnosis_Content\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"Old_Crxgsth_Id\",\"BizType\":1,\"Children\":[]}],\"DsId\":\"5694f8dd-0c32-4184-ba92-be2110600aaa\",\"IsCommon\":false,\"UsedExpressionID\":\"[]\"},{\"ID\":\"f2751f16-8870-2044-5248-81657d691499\",\"Name\":\"临床诊断\",\"UsedParametersId\":\"[]\",\"Configuration\":{\"AgentPars\":[],\"DSEnum\":0,\"CmdTimeOut\":30,\"SQL\":\"\",\"MaxRowCount\":-1},\"DataFields\":[],\"DsId\":\"4d26b4dd-44f5-475e-bd36-634cf24d57b5\",\"IsCommon\":false,\"UsedExpressionID\":\"[]\"},{\"ID\":\"e0f9dae6-c939-701e-a7d1-f650b133990f\",\"Name\":\"西药明细HIS\",\"UsedParametersId\":\"[\\\"CS\\\"]\",\"Configuration\":{\"AgentPars\":[{\"Name\":\"CS\",\"ParValue\":\"{P_CS}\",\"Format\":\"\",\"ParDirection\":1,\"ParType\":1}],\"DSEnum\":0,\"CmdTimeOut\":30,\"SQL\":\"SELECT * FROM (\\nSELECT a.相关ID,\\n       a.行号,\\n       MAX(a.药品1) AS 药品1,\\n       RTRIM(TO_CHAR(MAX(a.单次用量1), 'FM9990.9999'), '.')||MAX(a.计算单位1)  AS  总量1,\\n       MAX(a.药品2) AS 药品2,\\n       RTRIM(TO_CHAR(MAX(a.单次用量2), 'FM9990.9999'), '.')||MAX(a.计算单位2)  AS  总量2,\\n       MAX(a.脚注1) AS 脚注1,\\n       MAX(a.脚注2) AS 脚注2,\\n       --当时写这个是为了处理一张纸不知为何会写入“中药配方”字符的问题\\n       CASE WHEN b.医嘱内容 LIKE '%中药配方%' THEN SUBSTR(b.医嘱内容, INSTR(b.医嘱内容, ':', 1, 1) + 1, INSTR(b.医嘱内容, ':', 1, 2) - INSTR(b.医嘱内容, ':', 1, 1) - 1)\\n       ELSE SUBSTR(b.医嘱内容, INSTR(b.医嘱内容, ':', -1, 2) + 1, INSTR(b.医嘱内容, ':', -1, 1) - INSTR(b.医嘱内容, ':', -1, 2) - 1) END || ',' || B.医生嘱托 用法,\\n       decode (复诊,0,'√',null) 初诊,\\n       decode (复诊,1,'√',null) 复诊\\n  FROM (\\n  SELECT \\n               CEIL(序号 / 2) AS 行号,\\n               DECODE(MOD(序号 - 1, 2), 0, 药品) AS 药品1,\\n               DECODE(MOD(序号 - 1, 2), 0, 单次用量) AS 单次用量1,\\n               DECODE(MOD(序号 - 1, 2), 0, 计算单位) AS 计算单位1,\\n               \\n               DECODE(MOD(序号 - 1, 2), 1, 药品) AS 药品2,\\n               DECODE(MOD(序号 - 1, 2), 1, 单次用量) AS 单次用量2,\\n               DECODE(MOD(序号 - 1, 2), 1, 计算单位) AS 计算单位2,\\n\\n               DECODE(MOD(序号 - 1, 2), 0, 脚注) AS 脚注1,\\n               DECODE(MOD(序号 - 1, 2), 1, 脚注) AS 脚注2,\\n               相关id,\\n               复诊\\n          FROM (\\n          \\n       Select ROW_NUMBER() OVER(PARTITION BY a.相关ID ORDER BY a.序号) AS 序号,A.id,\\n               RPAD(I.名称, 20, ' ') As 药品,\\n               A.记录性质,\\n               /*decode(A.有费用,\\n                      1,\\n                      总数量,\\n                      A.总剂量 / decode(T.剂量系数, 0, 1, Null, 1, T.剂量系数)) As 总数量,*/\\n               A.单次用量, Z.计算单位,\\n               decode(T.门诊包装, 0, 1, Null, 1, T.门诊包装) As 门诊包装,\\n               T.门诊单位,\\n               decode(T.住院包装, 0, 1, Null, 1, T.住院包装) As 住院包装,\\n               T.住院单位,\\n               a.脚注,\\n               a.相关id,\\n               a.复诊\\n          From (Select A.相关id,\\n                       A.Id,\\n                       decode(E.收费细目id, Null, A.收费细目id, E.收费细目id) As 药品id,\\n                       decode(E.收费细目id, Null, 0, 1) As 有费用,\\n                       A.总剂量,\\n                       E.单次用量,\\n                       A.记录性质,\\n                       a.医生嘱托 as 脚注,\\n                       a.序号,\\n                       a.复诊\\n                  From (Select A.相关id,\\n                               A.Id,\\n                               A.收费细目id,\\n                               S.发送数次 As 总剂量,\\n                               S.记录性质,\\n                               a.医生嘱托,\\n                               a.序号,\\n                               c.复诊\\n                          from 病人医嘱记录 A,病人挂号记录 c,\\n                               (Select S.医嘱id, S.发送数次, S.记录性质\\n                                  From 病人医嘱发送 S\\n                                 Where S.No = Pljson(:CS).Get_String('Rcp_No')  /*and s.记录性质=b.病人来源*/) S\\n                         where A.ID = S.医嘱ID and A.执行性质 NOT IN (5)  and c.no=a.挂号单 and a.诊疗类别 in('7') and a.病人id=Pljson(:CS).Get_String('Pati_Id')) A,\\n                       (select E.id   as id,E.收费细目ID,E.单次用量, E.病人id,F.NO \\n                          from 病人医嘱记录 E,病人医嘱发送 F where \\n                          E.id=F.医嘱id \\n                          and E.诊疗类别 = '7' \\n                          and E.病人id=Pljson(:CS).Get_String('Pati_Id')\\n                          and F.No = Pljson(:CS).Get_String('Rcp_No')\\n                         ) E\\n                 Where A.Id = E.Id(+)) A,\\n               收费项目目录 I,\\n               药品规格 T,\\n               诊疗项目目录 Z,\\n                (select 药名id,是否皮试 from 药品特性 where 是否皮试='1')  B\\n         Where A.药品id = I.id\\n           And A.药品id = T.药品id\\n           and T.药名ID=B.药名ID(+)\\n           AND Z.ID = T.药名ID\\n                   \\n     union all \\n     \\n     Select ROW_NUMBER() OVER(PARTITION BY a.相关ID ORDER BY a.序号) AS 序号,A.id,\\n               RPAD(I.名称, 20, ' ') As 药品,\\n               A.记录性质,\\n               /*decode(A.有费用,\\n                      1,\\n                      总数量,\\n                      A.总剂量 / decode(T.剂量系数, 0, 1, Null, 1, T.剂量系数)) As 总数量,*/\\n               A.单次用量, Z.计算单位,\\n               decode(T.门诊包装, 0, 1, Null, 1, T.门诊包装) As 门诊包装,\\n               T.门诊单位,\\n               decode(T.住院包装, 0, 1, Null, 1, T.住院包装) As 住院包装,\\n               T.住院单位,\\n               a.脚注,\\n               a.相关id,\\n               a.复诊\\n          From (Select A.相关id,\\n                       A.Id,\\n                       decode(E.收费细目id, Null, A.收费细目id, E.收费细目id) As 药品id,\\n                       decode(E.收费细目id, Null, 0, 1) As 有费用,\\n                       A.总剂量,\\n                       E.单次用量,\\n                       A.记录性质,\\n                       a.医生嘱托 as 脚注,\\n                       a.序号,\\n                       a.复诊\\n                  From (Select A.相关id,\\n                               A.Id,\\n                               A.收费细目id,\\n                               S.发送数次 As 总剂量,\\n                               S.记录性质,\\n                               a.医生嘱托,\\n                               a.序号,\\n                               c.复诊\\n                          from 病人医嘱记录 A,病人挂号记录 c,\\n                               (Select S.医嘱id, S.发送数次, S.记录性质\\n                                  From 病人医嘱发送 S\\n                                 Where S.No = Pljson(:CS).Get_String('Rcp_No')  /*and s.记录性质=b.病人来源*/) S\\n                         where A.ID = S.医嘱ID and A.执行性质 NOT IN (5)  and c.no=a.挂号单 and a.诊疗类别 in('7') and a.病人id=Pljson(:CS).Get_String('Pati_Id')) A,\\n                       (select E.id   as id,E.收费细目ID,E.单次用量, E.病人id,F.NO \\n                          from 病人医嘱记录 E,病人医嘱发送 F where \\n                          E.id=F.医嘱id \\n                          and E.诊疗类别 = '7' \\n                          and E.病人id=Pljson(:CS).Get_String('Pati_Id')\\n                          and F.No = Pljson(:CS).Get_String('Rcp_No')\\n                         ) E\\n                 Where A.Id = E.Id(+)) A,\\n               收费项目目录 I,\\n               药品规格 T,\\n               诊疗项目目录 Z,\\n                (select 药名id,是否皮试 from 药品特性 where 是否皮试='1')  B\\n         Where A.药品id = I.id\\n           And A.药品id = T.药品id\\n           and T.药名ID=B.药名ID(+)\\n           AND Z.ID = T.药名ID)                   \\n                 ) a,病人医嘱记录 b\\n                 where a.相关id=b.id\\n GROUP BY a.行号,a.相关id,b.医嘱内容,复诊,复诊,B.医生嘱托\\n ORDER BY a.行号\\n )\\n \\n \\n UNION ALL \\n \\n \\n SELECT * FROM (\\n SELECT a.相关ID,\\n       a.行号,\\n       MAX(a.药品1) AS 药品1,\\n       RTRIM(TO_CHAR(MAX(a.单次用量1), 'fm9990.9999'), '.')||MAX(a.计算单位1)  AS  总量1,\\n       MAX(a.药品2) AS 药品2,\\n       RTRIM(TO_CHAR(MAX(a.单次用量2), 'fm9990.9999'), '.')||MAX(a.计算单位2)  AS  总量2,\\n       MAX(a.脚注1) AS 脚注1,\\n       MAX(a.脚注2) AS 脚注2,\\n       --substr(b.医嘱内容,1,instr(b.医嘱内容,':')-1) 用法,\\n       CASE WHEN b.医嘱内容 LIKE '%中药配方%' THEN SUBSTR(b.医嘱内容, INSTR(b.医嘱内容, ':', 1, 1) + 1, INSTR(b.医嘱内容, ':', 1, 2) - INSTR(b.医嘱内容, ':', 1, 1) - 1)\\nELSE SUBSTR(b.医嘱内容, INSTR(b.医嘱内容, ':', -1, 2) + 1, INSTR(b.医嘱内容, ':', -1, 1) - INSTR(b.医嘱内容, ':', -1, 2) - 1) END || ',' || B.医生嘱托 用法,\\n       null as  初诊,\\n       null as  复诊\\n  FROM (\\n  SELECT \\n               CEIL(序号 / 2) AS 行号,\\n               DECODE(MOD(序号 - 1, 2), 0, 药品) AS 药品1,\\n               DECODE(MOD(序号 - 1, 2), 0, 单次用量) AS 单次用量1,\\n               DECODE(MOD(序号 - 1, 2), 0, 计算单位) AS 计算单位1,\\n               \\n               DECODE(MOD(序号 - 1, 2), 1, 药品) AS 药品2,\\n               DECODE(MOD(序号 - 1, 2), 1, 单次用量) AS 单次用量2,\\n               DECODE(MOD(序号 - 1, 2), 1, 计算单位) AS 计算单位2,\\n\\n               DECODE(MOD(序号 - 1, 2), 0, 脚注) AS 脚注1,\\n               DECODE(MOD(序号 - 1, 2), 1, 脚注) AS 脚注2,\\n               相关id\\n          FROM (\\n          \\n       Select ROW_NUMBER() OVER(PARTITION BY a.相关ID ORDER BY a.序号) AS 序号,A.id,\\n               RPAD(I.名称, 20, ' ') As 药品,\\n               A.记录性质,\\n               /*decode(A.有费用,\\n                      1,\\n                      总数量,\\n                      A.总剂量 / decode(T.剂量系数, 0, 1, Null, 1, T.剂量系数)) As 总数量,*/\\n               A.单次用量, Z.计算单位,\\n               decode(T.门诊包装, 0, 1, Null, 1, T.门诊包装) As 门诊包装,\\n               T.门诊单位,\\n               decode(T.住院包装, 0, 1, Null, 1, T.住院包装) As 住院包装,\\n               T.住院单位,\\n               a.脚注,\\n               a.相关id,\\n               NULL AS 复诊\\n          From (Select A.相关id,\\n                       A.Id,\\n                       decode(E.收费细目id, Null, A.收费细目id, E.收费细目id) As 药品id,\\n                       decode(E.收费细目id, Null, 0, 1) As 有费用,\\n                       A.总剂量,\\n                       E.单次用量,\\n                       A.记录性质,\\n                       a.医生嘱托 as 脚注,\\n                       a.序号\\n                  From (Select A.相关id,\\n                               A.Id,\\n                               A.收费细目id,\\n                               S.发送数次 As 总剂量,\\n                               S.记录性质,\\n                               a.医生嘱托,\\n                               a.序号\\n                          from 病人医嘱记录 A,\\n                               (Select S.医嘱id, S.发送数次, S.记录性质\\n                                  From 病人医嘱发送 S\\n                                  JOIN 病人医嘱记录 S2 ON S.医嘱ID = S2.ID\\n                                 Where S.No = Pljson(:CS).Get_String('Rcp_No') AND S2.病人来源 = 2) S\\n                         where A.ID = S.医嘱ID and A.执行性质 NOT IN (5) and a.诊疗类别 in('7') and a.病人id=Pljson(:CS).Get_String('Pati_Id')) A,\\n                       (select E.id   as id,E.收费细目ID,E.单次用量, E.病人id,F.NO \\n                          from 病人医嘱记录 E,病人医嘱发送 F where \\n                          E.id=F.医嘱id \\n                          and E.诊疗类别 = '7' \\n                          and E.病人id=Pljson(:CS).Get_String('Pati_Id')\\n                          and F.No = Pljson(:CS).Get_String('Rcp_No')\\n                         ) E\\n                 Where A.Id = E.Id(+)) A,\\n               收费项目目录 I,\\n               药品规格 T,\\n               诊疗项目目录 Z,\\n                (select 药名id,是否皮试 from 药品特性 where 是否皮试='1')  B\\n         Where A.药品id = I.id\\n           And A.药品id = T.药品id\\n           and T.药名ID=B.药名ID(+)\\n           AND Z.ID = T.药名ID\\n                   \\n     union all \\n     \\n     Select ROW_NUMBER() OVER(PARTITION BY a.相关ID ORDER BY a.序号) AS 序号,A.id,\\n               RPAD(I.名称, 20, ' ') As 药品,\\n               A.记录性质,\\n               /*decode(A.有费用,\\n                      1,\\n                      总数量,\\n                      A.总剂量 / decode(T.剂量系数, 0, 1, Null, 1, T.剂量系数)) As 总数量,*/\\n               A.单次用量, Z.计算单位,\\n               decode(T.门诊包装, 0, 1, Null, 1, T.门诊包装) As 门诊包装,\\n               T.门诊单位,\\n               decode(T.住院包装, 0, 1, Null, 1, T.住院包装) As 住院包装,\\n               T.住院单位,\\n               a.脚注,\\n               a.相关id,\\n               a.复诊\\n          From (Select A.相关id,\\n                       A.Id,\\n                       decode(E.收费细目id, Null, A.收费细目id, E.收费细目id) As 药品id,\\n                       decode(E.收费细目id, Null, 0, 1) As 有费用,\\n                       A.总剂量,\\n                       E.单次用量,\\n                       A.记录性质,\\n                       a.医生嘱托 as 脚注,\\n                       a.序号,\\n                       a.复诊\\n                  From (Select A.相关id,\\n                               A.Id,\\n                               A.收费细目id,\\n                               S.发送数次 As 总剂量,\\n                               S.记录性质,\\n                               a.医生嘱托,\\n                               a.序号,\\n                               c.复诊\\n                          from 病人医嘱记录 A,病人挂号记录 c,\\n                               (Select S.医嘱id, S.发送数次, S.记录性质\\n                                  From 病人医嘱发送 S\\n                                  JOIN 病人医嘱记录 S2 ON S.医嘱ID = S2.ID\\n                                 Where S.No = Pljson(:CS).Get_String('Rcp_No') AND S2.病人来源 = 2) S\\n                         where A.ID = S.医嘱ID and A.执行性质 NOT IN (5)  and c.no=a.挂号单 and a.诊疗类别 in('7') and a.病人id=Pljson(:CS).Get_String('Pati_Id')) A,\\n                       (select E.id   as id,E.收费细目ID,E.单次用量, E.病人id,F.NO \\n                          from 病人医嘱记录 E,病人医嘱发送 F where \\n                          E.id=F.医嘱id \\n                          and E.诊疗类别 = '7' \\n                          and E.病人id=Pljson(:CS).Get_String('Pati_Id')\\n                          and F.No = Pljson(:CS).Get_String('Rcp_No')\\n                         ) E\\n                 Where A.Id = E.Id(+)) A,\\n               收费项目目录 I,\\n               药品规格 T,\\n               诊疗项目目录 Z,\\n                (select 药名id,是否皮试 from 药品特性 where 是否皮试='1')  B\\n         Where A.药品id = I.id\\n           And A.药品id = T.药品id\\n           and T.药名ID=B.药名ID(+)\\n           AND Z.ID = T.药名ID)                   \\n                 ) a,病人医嘱记录 b\\n                 where a.相关id=b.id\\n GROUP BY a.行号,a.相关id,b.医嘱内容,B.医生嘱托\\n ORDER BY a.行号\\n )\",\"MaxRowCount\":-1},\"DataFields\":[{\"ColumnName\":\"相关ID\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"行号\",\"BizType\":2,\"Children\":[]},{\"ColumnName\":\"药品1\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"总量1\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"药品2\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"总量2\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"脚注1\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"脚注2\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"用法\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"初诊\",\"BizType\":1,\"Children\":[]},{\"ColumnName\":\"复诊\",\"BizType\":1,\"Children\":[]}],\"DsId\":\"7496ecc7-7ced-4603-afa8-db9b236c3b34\",\"IsCommon\":false,\"IsLocalDataSet\":false,\"UsedExpressionID\":\"[]\"},{\"ID\":\"ca57711a-37dc-daf2-56d5-2a1b3fd33f4a\",\"Name\":\"信息HIS\",\"UsedParametersId\":\"[\\\"CS\\\"]\",\"Configuration\":{\"AgentPars\":[{\"Name\":\"CS\",\"ParValue\":\"{P_CS}\",\"Format\":\"\",\"ParDirection\":1,\"ParType\":1}],\"DSEnum\":0,\"CmdTimeOut\":30,\"SQL\":\"select distinct 姓名,性别,年龄,标识号,NO,开单科室,执行科室,开单人,开单人编号,审核人,配药人,核对人,发药人,地址,诊断,电话,住院号,床号,decode (复诊,0,'√',null)初诊,decode (复诊,1,'√',null)复诊, 医嘱期效\\nfrom (\\nselect \\na.姓名,\\na.性别,\\na.年龄,\\nd.门诊号 as 标识号,\\na.NO,\\nb.名称 as 开单科室,\\nc.名称 as 执行科室,\\nj.开嘱医生 as 开单人,\\ne.编号 as 开单人编号,\\na.审核人 as 审核人,\\na.配药人 as 配药人,\\na.核查人 as 核对人,\\na.取药确认人员 as 发药人,\\nd.家庭地址 as 地址,\\na.诊断描述 as 诊断,\\nd.家庭电话 as 电话,\\nd.住院号,\\nd.当前床号 as 床号,\\nf.复诊